{
  "ts-node": {
    "skipIgnore": true
  },
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "inlineSourceMap": true,
    "importHelpers": true,
    "experimentalDecorators": true
  }
}