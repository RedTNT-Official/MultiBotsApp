{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "module": "CommonJS",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "inlineSourceMap": true,
    "importHelpers": true,
    "experimentalDecorators": true,
    "declaration": true
  }
}